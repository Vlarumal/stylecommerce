{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "StyleCommerce": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=your-staging-db-host;Database=your_staging_database_name;Username=your_username;Password=your_password"
  },
  "JwtSettings": {
    "Secret": "your-staging-secret-key-that-should-be-at-least-32-characters-long",
    "Issuer": "YourApp.Api.Staging",
    "Audience": "YourApp.Api.Users.Staging",
    "ExpiryMinutes": 30
  },
  "SecuritySettings": {
    "AllowedCorsOrigins": [
      "https://staging.yourapp.example.com"
    ]
  }
}